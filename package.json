{
  "name": "msync",
  "version": "1.0.0",
  "description": "A message syncing application for facilitating omni-channel communication. Internal software for Collegiate. Cheaper than Slack. Easier than convincing clients to adopt Discord.",
  "main": "",
  "scripts": {
    "bot": "cd ./bot && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "server": "cd ./server && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "dev": "concurrently \"npm run bot\" \"npm run server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen-env": "concurrently \"cd ./server && gen-env-types .env -o src/env.d.ts -e .\" \"cd ./bot && gen-env-types .env -o src/env.d.ts -e .\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "discord.js": "^14.8.0",
    "dotenv-safe": "^8.2.0",
    "glob": "^9.3.5",
    "@prisma/client": "^5.0.0",
    "body-parser": "^1.20.2",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "twilio": "^4.9.0"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/glob": "^8.1.0",
    "@types/node": "^15.12.2",
    "gen-env-types": "^1.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^5.1.6",
    "concurrently": "^8.2.0",
    "tsc": "^2.0.4",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.4",
    "@types/uuid": "^8.3.0",
    "prisma": "^5.0.0"
  }
}
