    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@ngrok/ngrok": "^0.5.2",
    "@prisma/client": "^5.2.0",
    "@trpc/client": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "@types/body-parser": "^1.19.2",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
