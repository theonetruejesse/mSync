    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "discord.js": "^14.13.0",
    "dotenv-safe": "^8.2.0",
    "node-fetch": "^2.6.12",
    "twilio": "^4.15.0",
    "zod": "^3.22.1"
  }
}
